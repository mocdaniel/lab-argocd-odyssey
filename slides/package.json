{
  "name": "slidev-theme-nws",
  "version": "0.1.0",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.40.14"
  },
  "devDependencies": {
    "@iconify-json/fa6-brands": "^1.1.18",
    "@slidev/cli": "^0.40.14"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "dark",
    "highlighter": "shiki",
    "defaults": {
      "css": "unocss",
      "favicon": "./assets/images/favicon.ico",
      "fonts": {
        "sans": "Roboto",
        "mono": "Roboto Mono"
      },
      "hightlighter": "shiki"
    }
  }
}
